{
  "expo": {
    "name": "Wydad AC",
    "slug": "wydadapplication",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#BE1931"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wydadac.app",
      "config": {
        "googleMapsApiKey": "YOUR_GOOGLE_MAPS_API_KEY_HERE"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Cette application utilise votre position pour trouver les magasins Wydad AC les plus proches.",
        "NSLocationAlwaysUsageDescription": "Cette application utilise votre position pour trouver les magasins Wydad AC."
      }
    },
    "android": {
      "package": "com.wydadac.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#BE1931"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY_HERE"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permettre Ã  Wydad AC d'utiliser votre position pour trouver les magasins proches."
        }
      ]
    ]
  }
}
